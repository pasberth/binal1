(assume describe)
(assume it)
(let assert (. (require "chai") assert))

(let + (^ (s1 s2) (str.add s1 s2)))

(let desc (^ (x y) (describe (+ "" x) y)))

(desc "string" (^ () (seq
  (desc "s1 + s2" (^ () (seq
    (it "should be concatenated" (^ ()
      ((. assert equal) (+ "abc" "def") "abcdef")))))))))
